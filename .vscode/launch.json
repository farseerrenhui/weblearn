    "version": "0.2.0",
    "configurations": [{
            "name": "Chrome NoteBook",
            "type": "chrome",
            "request": "launch",
            "file": "${file}",
            "runtimeExecutable": "C:/Program Files (x86)/Google/Chrome/Application/chrome.exe",
            "sourceMaps": true,
            "webRoot": "${workspaceRoot}",
            "port": 5433
        },
        {
            "name": "Chrome HomePC",
            "type": "chrome",
            "request": "launch",
            "file": "${file}",
            "runtimeExecutable": "C:/Users/Administrator/AppData/Local/Google/Chrome/Application/chrome.exe",
            "sourceMaps": true,
            "webRoot": "${workspaceRoot}",
            "port": 5433
        },
        {
            "type": "firefox",
            "request": "launch",
            "reAttach": true,
            "name": "Firefox Debug",
            "file": "${file}",
            "firefoxExecutable": "E:/App/Mozilla Firefox/firefox.exe"
        },
    ]
}